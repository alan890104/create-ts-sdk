{
  "name": "create-ts-sdk",
  "version": "1.1.0",
  "description": "Create typescript SDK for frontend and backend",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "bin": {
    "create-ts-sdk": "./dist/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production tsup --config tsup.config.ts",
    "dev": "NODE_ENV=development tsup --config tsup.config.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "release": "changeset",
    "ci:publish": "pnpm build && changeset publish --access public",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "inquirer": "^12.3.0",
    "ora": "^8.1.1"
  }
}
